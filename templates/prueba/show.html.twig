{% extends 'base.html.twig' %}

{% block title %}Prueba{% endblock %}

{% block body %}
    <h1>Prueba</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prueba.id }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ prueba.titulo }}</td>
            </tr>
            <tr>
                <th>Texto</th>
                <td>{{ prueba.texto }}</td>
            </tr>
            <tr>
                <th>Indexdate</th>
                <td>{{ prueba.indexdate ? prueba.indexdate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>PublishStartDate</th>
                <td>{{ prueba.publishStartDate ? prueba.publishStartDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('prueba_index') }}">back to list</a>

    <a href="{{ path('prueba_edit', {'id': prueba.id}) }}">edit</a>

    {{ include('prueba/_delete_form.html.twig') }}
{% endblock %}
